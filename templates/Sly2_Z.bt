typedef struct Object_t {
    char fk_str[4];
    char name[60];

    //Printf("%40s ", name);
    //hexdump(FTell(), 6*2);

    uint32 unk_u32_0;
    uint32 unk_u32_1;
    uint32 unk_u32_2;
    uint32 unk_u32_3;
    uint32 count;

    Printf("%X | %X | %08X | %08X | %02X | %s | %s\n",
        unk_u32_0, unk_u32_1, unk_u32_2, unk_u32_3, count, fk_str, name);
} Object <read=read_Object, optimize=false>;
string read_Object(Object &v) {
    string s;
    SPrintf(s, "%X | %X | %08X | %08X | %02X | %s",
        v.unk_u32_0, v.unk_u32_1, v.unk_u32_2, v.unk_u32_3, v.count, v.name);
    return s;
}

